{#
/**
 * @file song.html.twig
 * Default theme implementation to present Song data.
 *
 * This template is used when viewing Song pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_song()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('song') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
